{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|images|blog|tools)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/(analyze-seo|dns-lookup|pagespeed|ping|chat)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "api/server.js": {
      "includeFiles": "tools-database.json"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
